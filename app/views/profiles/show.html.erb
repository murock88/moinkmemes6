



<% @memes.each do |meme| %>
    <%= image_tag meme.image.url(:medium) %> <br>
        <td><a href="/profiles/<%= User.find(meme.user_id).id %>"><%= meme.user.email %></td>
        <td><%= link_to 'Show', meme %></td>
        <td><%= link_to 'Edit', edit_meme_path(meme) %></td>
        <td><%= link_to 'Destroy', meme, method: :delete, data: { confirm: 'Are you sure?' } %></td>
 <% end %>




<li> <%= current_user.followers.count %>
<li> <%= current_user.following.count %>

<%= render '/components/follow_button', :user => User.find_by_id(params[:id]) %>

                    